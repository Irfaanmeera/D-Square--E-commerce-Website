<header class="header navbar-area">
  <!-- Start Topbar -->
<div class="topbar">
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-2 col-md-4 col-12">
        <div class="top-left">
          <h3 style="color: rgba(255, 255, 255, 0.96)">D SQUARE</h3>
        </div>
      </div>
      <div class="col-lg-4 col-md-5 col-12 d-xs-none">
        <!-- Start Main Menu Search -->
        <form id="searchForm" onsubmit="event.preventDefault(); performSearch();">
          <div class="main-menu-search">
            <div class="navbar-search">
              <div class="search-input">
                <input id="searchInput" type="text" placeholder="Search" />
              </div>
              <div class="search-btn">
                <button type="submit"><i class="lni lni-search-alt"></i></button>
              </div>
            </div>
          </div>
        </form>
        
        <!-- End Main Menu Search -->
      </div>
      <!-- Navbar Right -->
      {{#if user}}
      <div class="col-lg-4 col-md-8 col-12">
        <div class="top-end">
          <div class="user">
            <nav class="navbar navbar-expand-lg">
              <button class="navbar-toggler mobile-menu-btn" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                <ul id="nav" class="navbar-nav ms-auto">
                  <li class="nav-item">
                    <i class="lni lni-user"></i>
                    <a class="dd-menu collapsed" href="javascript:void(0)" data-bs-toggle="collapse" data-bs-target="#submenu-1-3" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation" style="color:#ffffff;">Hello, {{user.name}}</a>
                    <ul class="sub-menu collapses" style="padding-right: -50px;" id="submenu-1-3">
                      <li class="nav-item">
                        <a href="/userAccount">My Profile</a>
                      </li>
                      <li class="nav-item">
                        <a href="/wishlist">Wishlist</a>
                      </li>
                      <li class="nav-item">
                        <a href="/orderList">My Orders</a>
                      </li>
                      <li class="nav-item">
                        <button class="btn btn-primary"><a href="/logout" style="color: #ffffff;">Log Out</a></button>
                      </li>
                    </ul>
                  </li>
                </ul>
              </div>
            </nav>
          </div>
        </div>
      </div>
      <div class="col-lg-2 col-md-2 col-12">
        <div class="navbar-cart">
          <div class="wishlist">
            <a href="/wishlist">
              <i class="fa-regular fa-heart" style="color: #ffffff;width:50px; height:50px;"></i>
              {{#if wishlistData}}
              <span class="total-items">{{wishlistCount}}</span>
              {{/if}}
            </a>
          </div>
          <div class="cart-items">
            <a href="/cart" class="main-btn">
              <i class="fa-solid fa-cart-shopping" style="color: #ffffff; width:30px; height:30px;"></i>
              {{#if cartProduct}}
              <span class="total-items">{{count}}</span>
              {{/if}}
            </a>
          </div>
        </div>
      </div>
      {{else}}
      <div class="col-lg-4 col-md-4 col-12">
        <div class="top-end">
          <div class="user">
            <i class="lni lni-user"></i>
            Hello, User
          </div>
          <ul class="user-login">
            <li>
              <a href="/login">Sign In</a>
            </li>
            <li>
              <a href="/signup">Register</a>
            </li>
          </ul>
        </div>
      </div>
      {{/if}}
      <!-- /Navbar Right -->
    </div>
  </div>
</div>

  </div>
  <!-- End Topbar -->

  <!-- Start Header Bottom -->
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-8 col-md-6 col-12">
        <div class="nav-inner">
          <!-- Start Mega Category Menu -->
          <div class="mega-category-menu">
            <span class="cat-button"><i class="lni lni-menu"></i>All Categories</span>
            <ul class="sub-category">
              {{#each categoryData}}
              <li>
                <a href="/filterCategory/{{this.categoryName}}">
                  <label class="tick">{{this.categoryName}}</label>
                </a>
              </li>
              {{/each}}
            </ul>
          </div>
          <!-- End Mega Category Menu -->
          <!-- Start Navbar -->
          <nav class="navbar navbar-expand-lg">
            <button class="navbar-toggler mobile-menu-btn" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
              <span class="toggler-icon"></span>
              <span class="toggler-icon"></span>
              <span class="toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
              <ul id="nav" class="navbar-nav ms-auto">
                <li class="nav-item">
                  <a href="/" aria-label="Toggle navigation">Home</a>
                </li>
                <li class="nav-item">
                  <a href="/shop" aria-label="Toggle navigation">Shop</a>
                </li>
                <li class="nav-item">
                  <a class="dd-menu collapsed" href="javascript:void(0)" data-bs-toggle="collapse" data-bs-target="#submenu-1-4" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">Blog</a>
                  <ul class="sub-menu collapse" id="submenu-1-4">
                    <li class="nav-item"><a href="blog-grid-sidebar.html">Blog Grid Sidebar</a></li>
                    <li class="nav-item"><a href="blog-single.html">Blog Single</a></li>
                    <li class="nav-item"><a href="blog-single-sidebar.html">Blog Single Sibebar</a></li>
                  </ul>
                </li>
                <li class="nav-item">
                  <a href="contact.html" aria-label="Toggle navigation">Contact Us</a>
                </li>
              </ul>
            </div>
            <!-- navbar collapse -->
          </nav>
          <!-- End Navbar -->
        </div>
      </div>
    </div>
  </div>
  <!-- End Header Bottom -->
</header>
